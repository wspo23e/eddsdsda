							OldCoreTypeSettings[CoreType] = Enabled
							return
						end
					elseif Method == "SetCore" then
						local Core = Arguments[1]
						local Connection = Arguments[2]
	
						if Core == "CoreGuiChatConnections" then
							OldCoreSetting = Connection
							return
						end
					end
				end
	
				return CoreHook(self, ...)
			end))
